{
    "name": "simple tab manager",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
            ]
        }
    ],
    "action": {
        "default_popup": "./dist/index.html"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
    ]
}
